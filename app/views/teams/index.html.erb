<div class='row'>
	<% @teams.each do |team| %>
		<div class='team-card col-md-4'>
			<div class='color'>
				<a href='<%=team.url%>' class='team-name'><%= team.name %></a>
				<br>
				<p class='team-count'><%= team.woman_count %> out of <%= team.team_count %></p>
				<p class='percent'><%= team.percent%>%</p>
			</div>
		</div>
	<% end %>
</div>